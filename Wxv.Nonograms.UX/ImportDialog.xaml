<Window x:Class="Wxv.Nonograms.UX.ImportDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:local="clr-namespace:Wxv.Nonograms.UX"
        mc:Ignorable="d"
        Title="Paste" 
        Height="125" 
        Width="425"
        Loaded="ImportDialog_OnLoaded"
        Unloaded="ImportDialog_OnUnloaded"
        ResizeMode="NoResize">
    <Grid>
        <ProgressBar x:Name="ProgressBar" HorizontalAlignment="Stretch" Height="16" Margin="0,10,0,0" VerticalAlignment="Top" Width="400" Value="0" />
        <TextBox x:Name="ProgressTextBox" HorizontalAlignment="Stretch" Margin="0,31,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="400" TextAlignment="Center" />
        <Button x:Name="CancelButton" Content="Cancel" HorizontalAlignment="Right" Margin="0,55,13,0" VerticalAlignment="Top" Width="64" Click="CancelButton_Click"/>
    </Grid>
</Window>
